<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - AKDEMIR Üniversitesi</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/GraduateAppProject.MVC.styles.css" asp-append-version="true" />
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.10.5/font/bootstrap-icons.css">
    <link rel="icon" type="image/x-icon" href="~/img/favicon.svg" />
    <link href="~/alertifyjs/css/alertify.min.css" rel="stylesheet" />
    
</head>
<body class="d-flex flex-column min-vh-100 h-100 mb-0">
    <header class="sticky-top bg-dark">
        <partial name="_Navbars"></partial>
    </header>
    <content>
        <main role="main">
            @RenderBody()
        </main>
    </content>

    @*<footer class="align-bottom">
        <div class="container-fluid">
            <partial name="_Footers"></partial>
        </div>
    </footer>*@
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    <script src="~/alertifyjs/alertify.min.js"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
